<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodel="clr-namespace:deinbaumApp.ViewModel.Startup"
             x:DataType="viewmodel:LoadingPageViewModel"
             x:Class="deinbaumApp.Views.Startup.LoadingPage"
             Title="{Binding Title}">
    <StackLayout>
        <ActivityIndicator IsRunning="True" HeightRequest="40" WidthRequest="40" HorizontalOptions="Center" VerticalOptions="Center" />
    </StackLayout>
</ContentPage>